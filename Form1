                End Select
            Case 8

                'AAEPD and EOL

            Case Else
                MsgBox("Damn son, you have an error right here. Please let Hollowchick know his program is going haywire for no fucking reason.", , "Critical Error.")
        End Select
        ModlistP.Clear()
        ModlistY.Clear()
        ModlistR.Clear()
        ModlistB.Clear()
        Modlistcheck = 0
    End Sub

    'Click Events

    Private Sub picWeapon_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picWeapon.Click
        If StatWeapon = False Then
            StatWeapon = True
            picWeapon.BackgroundImage = My.Resources.Background_ModBorderSelected
            picWeapon.Image = My.Resources.Empty
        ElseIf StatWeapon = True Then
            StatWeapon = False
            picWeapon.BackgroundImage = My.Resources.Background_ModBorder
            picWeapon.Image = My.Resources.None
        End If
    End Sub

    Private Sub picMod1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picMod1.Click
        If StatMod1 = False Then
            StatMod1 = True
            picMod1.BackgroundImage = My.Resources.Background_ModBorderSelectedThick
            picMod1.Image = My.Resources.Empty
        ElseIf StatMod1 = True Then
            StatMod1 = False
            picMod1.BackgroundImage = My.Resources.Background_ModBorderThick
            picMod1.Image = My.Resources.None
        End If
    End Sub

    Private Sub picMod2_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picMod2.Click
        If StatMod2 = False Then
            StatMod2 = True
            picMod2.BackgroundImage = My.Resources.Background_ModBorderSelectedThick
            picMod2.Image = My.Resources.Empty
