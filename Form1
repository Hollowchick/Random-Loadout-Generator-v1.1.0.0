        'Challenge Randomization
        If My.Settings.Challenges = 1 Then
            Challenge = generator.Next(1, 4)
            If Challenge = 1 Then
                ChallengeInt = generator.Next(1, 25)
                Select Case ChallengeInt
                    Case 1
                        ChallengeLabel1.Text = "It's in the workshop!"
                        ChallengeLabel2.Text = "Unequip your player vehicle"
                    Case 2
                        ChallengeLabel1.Text = "You can't buy a new car for 100$!"
                        ChallengeLabel2.Text = "Don't use car spawn stations!"
