                                            Loop Until ModlistR.Contains(GunMod2)
                                        End If
                                        picMod2.Image = My.Resources.ResourceManager.GetObject("red_" & GunMod2.ToString())
                                    End If
                                End If
                                If Modlistcheck < 2 Then
                                    picMod3.Image = My.Resources.Empty
