                        ChallengeLabel2.Text = ""
                End Select
            Else
                ChallengeLabel1.Text = ""
                ChallengeLabel2.Text = ""
            End If
        Else
            ChallengeLabel1.Text = ""
            ChallengeLabel2.Text = ""
        End If


        'Empty Slot Chance
        If EmptySlot = 1 Then
            EmptyRandom = generator.Next(1, 16)

            Select Case EmptyRandom
                Case 1
                    If StatWeapon = True Then
                        picWeapon.Image = My.Resources.Empty
                        picMod1.Image = My.Resources.Empty
                        picMod2.Image = My.Resources.Empty
                        picMod3.Image = My.Resources.Empty
                    End If
                Case 2
                    If StatSec = True And StatNade = True Then
                        picSecondary.Image = My.Resources.Empty
                        picNade.Image = My.Resources.Empty
                    End If
                Case 3
                    If StatCharBlue = True And StatCharGreen = True Then
                        picCharModBlue.Image = My.Resources.Empty
                        picCharModGreen.Image = My.Resources.Empty
                    End If
            End Select
        End If


        'Hard Mode
        If My.Settings.HardMode = 1 Then
            'Mod Changes

            'Increased chance for HS on Assault Rifles to get RS or HMS
            Select Case WeaponCategory
                Case 1
                    If Color1 = "2" Or Color2 = "2" Or Color3 = "2" Then
                        HardModeInt = generator.Next(1, 5)
                        If HardModeInt = "1" And ModlistY.Contains("5" Or "6" Or "7") Then
                            If Color1 = "2" Then
                                Do
                                    GunMod1 = generator.Next(5, 8)
                                Loop Until ModlistY.Contains(GunMod1)
                                picMod1.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod1.ToString())
                            ElseIf Color2 = "2" Then
                                Do
                                    GunMod2 = generator.Next(5, 8)
                                Loop Until ModlistY.Contains(GunMod2)
                                picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                            ElseIf Color3 = "2" Then
                                Do
                                    GunMod3 = generator.Next(5, 8)
                                Loop Until ModlistY.Contains(GunMod3)
                                picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                            End If
                        ElseIf HardModeInt = "2" Then
                            If ModlistY.Contains("1") Then
                                If Color1 = "2" Then
                                    picMod1.Image = My.Resources.yellow_1
                                ElseIf Color2 = "2" Then
                                    picMod2.Image = My.Resources.yellow_1
                                ElseIf Color3 = "2" Then
                                    picMod3.Image = My.Resources.yellow_1
                                End If
                            End If
                        End If
                    End If
                    'ACES Rifle higher Mag Pull Chance
                    If Primary = "2" And (Color2 = "4" Or Color3 = "4") Then
                        HardModeInt = generator.Next(1, 3)
                        If HardModeInt = "1" And ModlistB.Contains("7" Or "8" Or "9") Then
                            If Color2 = "4" Then
                                Do
                                    GunMod2 = generator.Next(7, 10)
                                Loop Until ModlistB.Contains(GunMod2)
                                picMod2.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod2.ToString())
                            ElseIf Color3 = "4" Then
                                Do
                                    GunMod3 = generator.Next(7, 10)
                                Loop Until ModlistB.Contains(GunMod3)
                                picMod3.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod3.ToString())
                            End If
                        End If
                    End If

        'Shotguns SMGs less RS
                Case 4 And 5
                    If Color1 = "2" Or Color2 = "2" Or Color3 = "2" Then
                        HardModeInt = generator.Next(1, 4)
                        If HardModeInt = "1" And ModlistY.Contains("2" Or "3" Or "4") Then
                            If Color1 = "2" Then
                                Do
                                    GunMod1 = generator.Next(2, 5)
                                Loop Until ModlistY.Contains(GunMod1)
