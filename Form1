                                ElseIf Color3 = "4" Then
                                    Do
                                        GunMod3 = generator.Next(7, 10)
                                    Loop Until ModlistB.Contains(GunMod3)
                                    picMod3.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod3.ToString())
                                End If
                            End If
                        End If
                    End If
                    'Sniper higher chance for RS
                Case 6
                    If Color1 = "2" Or Color2 = "2" Or Color3 = "2" Then
                        If (GunMod1 = "1" Or GunMod1 = "2" Or GunMod1 = "3" Or GunMod1 = "4") And Color1 = "2" Then
                            GunMod1 = generator.Next(5, 8)
                            picMod1.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod1.ToString())
                        ElseIf (GunMod2 = "1" Or GunMod2 = "2" Or GunMod2 = "3" Or GunMod2 = "4") And Color2 = "2" Then
                            GunMod2 = generator.Next(5, 8)
                            picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                        ElseIf (GunMod3 = "1" Or GunMod3 = "2" Or GunMod3 = "3" Or GunMod3 = "4") And Color3 = "2" Then
                            GunMod3 = generator.Next(5, 8)
                            picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                        End If
                    End If
            End Select
            If Color1 = "2" Or Color2 = "2" Or Color3 = "2" Then
                HardModeInt = generator.Next(1, 4)
                If HardModeInt = "1" Then
                    If Color1 = "2" Then
                        picMod1.Image = My.Resources.yellow_1
                    ElseIf Color2 = "2" Then
                        picMod2.Image = My.Resources.yellow_1
                    ElseIf Color3 = "2" Then
                        picMod3.Image = My.Resources.yellow_1
                    End If
                End If
            End If

        End If
        'Routines for preslotted Gunmods 

PreslotModsAgain:

        Modlistcheck = 0
        If ModlistP.Count <> 0 Then
            Modlistcheck += 1
        End If
        If ModlistY.Count <> 0 Then
            Modlistcheck += 1
        End If
        If ModlistR.Count <> 0 Then
            Modlistcheck += 1
        End If
        If ModlistB.Count <> 0 Then
            Modlistcheck += 1
        End If

        Select Case WeaponCategory
            Case 0
                picWeapon.Image = My.Resources.None

                'Assault Rifles
            Case 1
                'ACES Rifle Routine
                If Primary = 2 Then
                    If Color2 = "3" Or Color3 = "3" Then
                        If ModlistR.Sum <> 0 Then
                            Modlistcheck -= 1
                        End If
                        If Modlistcheck < 1 Then
                            picMod2.Image = My.Resources.Empty
                        Else
                            Color2 = generator.Next(1, 4)
                            If Color2 = "1" Then
                                If ModlistP.Count = 0 Then
                                    Color2 = 3
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod2 = generator.Next(1, 6)
                                    Loop Until ModlistP.Contains(GunMod2)
                                End If
                                picMod2.Image = My.Resources.ResourceManager.GetObject("purple_" & GunMod2.ToString())
                            ElseIf Color2 = "2" Then
                                If ModlistY.Count = 0 Then
                                    Color2 = 3
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod2 = generator.Next(1, 8)
                                    Loop Until ModlistY.Contains(GunMod2)
                                End If
                                picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                            ElseIf Color2 = "3" Then
                                If ModlistB.Count = 0 Then
                                    Color2 = 3
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod2 = generator.Next(1, 10)
                                    Loop Until ModlistB.Contains(GunMod2)
                                End If
                                picMod2.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod2.ToString())
                            End If
                        End If
                        If Modlistcheck < 2 Then
                            picMod3.Image = My.Resources.Empty
                        Else
                            Do
                                Color3 = generator.Next(1, 4)
                            Loop Until Color3 <> Color2
                            If Color3 = "1" Then
                                If ModlistP.Count = 0 Then
                                    Color3 = 3
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod3 = generator.Next(1, 6)
                                    Loop Until ModlistP.Contains(GunMod3)
                                End If
                                picMod3.Image = My.Resources.ResourceManager.GetObject("purple_" & GunMod3.ToString())
                            ElseIf Color3 = "2" Then
                                If ModlistY.Count = 0 Then
                                    Color3 = 3
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod3 = generator.Next(1, 8)
                                    Loop Until ModlistY.Contains(GunMod3)
                                End If
                                picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                            ElseIf Color3 = "3" Then
                                If ModlistB.Count = 0 Then
                                    Color3 = 3
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod3 = generator.Next(1, 10)
                                    Loop Until ModlistB.Contains(GunMod3)
                                End If
                                picMod3.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod3.ToString())
                            End If
                        End If
                    End If
                    picMod1.Image = My.Resources.extendedBarrel
                    'Raptor 45 Routine
                ElseIf Primary = 9 Then
                    If Color1 = "2" Or Color1 = "3" Or Color1 = "4" Then
                        picMod2.Image = picMod1.Image
                    End If
                    If Color2 = "1" Then
                        If Modlistcheck < 1 Then
                            picMod2.Image = My.Resources.Empty
                        Else
                            Color2 = generator.Next(1, 4)
                            If Color2 = "1" Then
                                If ModlistY.Count = 0 Then
                                    Color2 = 1
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod2 = generator.Next(1, 8)
                                    Loop Until ModlistY.Contains(GunMod2)
                                End If
                                picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                            ElseIf Color2 = "2" Then
                                If ModlistR.Count = 0 Then
                                    Color2 = 1
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod2 = generator.Next(1, 11)
                                    Loop Until ModlistR.Contains(GunMod2)
                                End If
                                picMod2.Image = My.Resources.ResourceManager.GetObject("red_" & GunMod2.ToString())
                            ElseIf Color2 = "3" Then
                                If ModlistB.Count = 0 Then
                                    Color2 = 1
                                    GoTo PreslotModsAgain
                                Else
                                    Do
                                        GunMod2 = generator.Next(1, 10)
                                    Loop Until ModlistB.Contains(GunMod2)
                                End If
                                picMod2.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod2.ToString())
                            End If
                        End If
                    End If
                    picMod1.Image = My.Resources.silencerp
                    picMod3.Image = My.Resources.None
                    'Star LCR Routine
                ElseIf Primary = 11 Then
                    picMod1.Image = My.Resources.yellow_4
                    picMod2.Image = My.Resources.red_9
                    picMod3.Image = My.Resources.None
                End If
                'LMGs
            Case 2
                'AMG Routine
                If Primary = 7 Then
                    picMod3.Image = My.Resources.None
                End If
                'Rifles
            Case 3
                'Tempress Routine
                If Primary = 6 Then
                    picMod1.Image = My.Resources.silencerp
                    picMod2.Image = My.Resources.yellow_4
                    picMod3.Image = My.Resources.red_9
                End If
                'Shotguns
            Case 4
                'Shredder Routine
