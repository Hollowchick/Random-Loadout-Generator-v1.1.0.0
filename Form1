            picSecondary.BackgroundImage = My.Resources.Background_ModBorderSelected
            picSecondary.Image = My.Resources.Empty
        ElseIf StatSec = True Then
            StatSec = False
            picSecondary.BackgroundImage = My.Resources.Background_ModBorder
            picSecondary.Image = My.Resources.None
        End If
    End Sub

    Private Sub picNade_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picNade.Click
        If StatNade = False Then
            StatNade = True
            picNade.BackgroundImage = My.Resources.Background_ModBorderSelected
            picNade.Image = My.Resources.Empty
        ElseIf StatNade = True Then
            StatNade = False
            picNade.BackgroundImage = My.Resources.Background_ModBorder
            picNade.Image = My.Resources.None
        End If
    End Sub

    Private Sub Button2_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button2.Click
        Form2.Show()
    End Sub
