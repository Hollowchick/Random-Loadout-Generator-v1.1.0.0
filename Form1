Public Class Form1
    'Boolean Variable Status Declaration
    Dim StatWeapon As Boolean = True
    Dim StatMod1 As Boolean = True
    Dim StatMod2 As Boolean = True
    Dim StatMod3 As Boolean = True
    Dim StatNade As Boolean = False
    Dim StatSec As Boolean = False
    Dim StatCharGreen As Boolean = False
    Dim StatCharBlue As Boolean = False

    Private Sub Button1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button1.Click
        Dim generator As New Random
        Dim Color1, Color2, Color3 As Integer
        Dim EmptySlot As Integer
        Dim ModlistP As New List(Of Integer)
        Dim ModlistY As New List(Of Integer)
        Dim ModlistR As New List(Of Integer)
        Dim ModlistB As New List(Of Integer)
        Dim Modlistcheck As Integer
        Dim GunList As New List(Of Integer)
        Dim NadeList As New List(Of Integer)
        Dim CharList As New List(Of Integer)
        Dim Count As Integer = 4
        Dim GunMod1, GunMod2, GunMod3 As Integer
        Dim WeaponCategoryInt1, WeaponCategoryInt2, WeaponCategoryInt3, WeaponCategoryInt4, WeaponCategoryInt5, WeaponCategoryInt6, WeaponCategoryInt7, WeaponCategoryInt8, ChallengeInt, HardModeInt As Integer

        Dim WeaponCategory, Primary, Secondary, Nade, CharGreen, CharBlue, Challenge, EmptyRandom As Integer

        ' Purple = 1
        ' Yellow = 2
        ' Red = 3
        ' Blue = 4

        'Empty Mod Slots
        My.Settings.Reload()

        EmptySlot = My.Settings.EmptyMods

        picMod1.Image = My.Resources.None
        picMod2.Image = My.Resources.None
        picMod3.Image = My.Resources.None

        If EmptySlot = 1 Then
            Color1 = generator.Next(1, 6)
            Color2 = generator.Next(1, 6)
            Color3 = generator.Next(1, 6)
        Else
            Color1 = generator.Next(1, 5)
            Color2 = generator.Next(1, 5)
            Color3 = generator.Next(1, 5)
        End If
        'Loop Prevention + Comparing Colors

        My.Settings.Reload()
        If My.Settings.ModsPNo = 1 Then
            Count -= 1
        End If
        If My.Settings.ModsYNo = 1 Then
            Count -= 1
        End If
        If My.Settings.ModsRNo = 1 Then
            Count -= 1
        End If
        If My.Settings.ModsBNo = 1 Then
            Count -= 1
        End If
        If Count = 0 Then
            Color1 = 0
            Color2 = 0
            Color3 = 0
        ElseIf Count = 1 Then
            Color2 = 0
            Color3 = 0
        ElseIf Count = 2 Then
            If Color2 = Color1 Then
                If EmptySlot = 1 Then
                    If Color2 = 5 Then
                    Else
                        Do
                            Color2 = generator.Next(1, 6)
                        Loop While Color2 = Color1 Or Color2 = 5
                    End If
                Else
                    Do
                        Color2 = generator.Next(1, 5)
                    Loop While Color2 = Color1
                End If
            End If
            Color3 = 0
        Else
            'Comparing Colors
            If Color2 = Color1 Then
                If EmptySlot = 1 Then
                    If Color2 = 5 Then
                    Else
                        Do
                            Color2 = generator.Next(1, 6)
                        Loop While Color2 = Color1
                    End If
                Else
                    Do
                        Color2 = generator.Next(1, 5)
                    Loop While Color2 = Color1
                End If
            End If
            If Color3 = Color2 Or Color3 = Color1 Then
                If EmptySlot = 1 Then
                    If Color3 = 5 Then
                    Else
                        Do
                            Color3 = generator.Next(1, 6)
                        Loop While Color3 = Color2 Or Color3 = Color1
                    End If
                Else
                    Do
                        Color3 = generator.Next(1, 5)
                    Loop While Color3 = Color2 Or Color3 = Color1
                End If
            End If
        End If

        If StatMod1 = False Then
            Color1 = "0"
        End If
        If StatMod2 = False Then
            Color2 = "0"
        End If
        If StatMod3 = False Then
            Color3 = "0"
        End If

        Count = 0
        ModlistP.Clear()
        ModlistY.Clear()
        ModlistR.Clear()
        ModlistB.Clear()

        If My.Settings.ModsP1 = 1 Then
            ModlistP.Add(1)
        End If
        If My.Settings.ModsP2 = 1 Then
            ModlistP.Add(2)
        End If
        If My.Settings.ModsP3 = 1 Then
            ModlistP.Add(3)
        End If
        If My.Settings.ModsP4 = 1 Then
            ModlistP.Add(4)
        End If
        If My.Settings.ModsP5 = 1 Then
            ModlistP.Add(5)
        End If

        If My.Settings.ModsY1 = 1 Then
            ModlistY.Add(1)
        End If
        If My.Settings.ModsY2 = 1 Then
            ModlistY.Add(2)
        End If
        If My.Settings.ModsY3 = 1 Then
            ModlistY.Add(3)
        End If
        If My.Settings.ModsY4 = 1 Then
            ModlistY.Add(4)
        End If
        If My.Settings.ModsY5 = 1 Then
            ModlistY.Add(5)
        End If
        If My.Settings.ModsY6 = 1 Then
            ModlistY.Add(6)
        End If
        If My.Settings.ModsY7 = 1 Then
            ModlistY.Add(7)
        End If

        If My.Settings.ModsR1 = 1 Then
            ModlistR.Add(1)
        End If
        If My.Settings.ModsR2 = 1 Then
            ModlistR.Add(2)
        End If
        If My.Settings.ModsR3 = 1 Then
            ModlistR.Add(3)
        End If
        If My.Settings.ModsR4 = 1 Then
            ModlistR.Add(4)
        End If
        If My.Settings.ModsR5 = 1 Then
            ModlistR.Add(5)
        End If
        If My.Settings.ModsR6 = 1 Then
            ModlistR.Add(6)
        End If
        If My.Settings.ModsR7 = 1 Then
            ModlistR.Add(7)
        End If
        If My.Settings.ModsR8 = 1 Then
            ModlistR.Add(8)
        End If
        If My.Settings.ModsR9 = 1 Then
            ModlistR.Add(9)
        End If
        If My.Settings.ModsR10 = 1 Then
            ModlistR.Add(10)
        End If

        If My.Settings.ModsB1 = 1 Then
            ModlistB.Add(1)
        End If
        If My.Settings.ModsB2 = 1 Then
            ModlistB.Add(2)
        End If
        If My.Settings.ModsB3 = 1 Then
            ModlistB.Add(3)
        End If
        If My.Settings.ModsB4 = 1 Then
            ModlistB.Add(4)
        End If
        If My.Settings.ModsB5 = 1 Then
            ModlistB.Add(5)
        End If
        If My.Settings.ModsB6 = 1 Then
            ModlistB.Add(6)
        End If
        If My.Settings.ModsB7 = 1 Then
            ModlistB.Add(7)
        End If
        If My.Settings.ModsB8 = 1 Then
            ModlistB.Add(8)
        End If
        If My.Settings.ModsB9 = 1 Then
            ModlistB.Add(9)
        End If

ColorAgain:
        'Color1
        My.Settings.Reload()
        Select Case Color1
            Case 1
                If ModlistP.Count = 0 Then
                    Do
                        Color1 = generator.Next(1, 5)
                    Loop Until Color1 <> "1" And Color1 <> Color2 And Color1 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod1 = generator.Next(1, 6)
                    Loop Until ModlistP.Contains(GunMod1)
                    picMod1.Image = My.Resources.ResourceManager.GetObject("purple_" & GunMod1.ToString())
                End If
            Case 2
                If ModlistY.Count = 0 Then
                    Do
                        Color1 = generator.Next(1, 5)
                    Loop Until Color1 <> "2" And Color1 <> Color2 And Color1 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod1 = generator.Next(1, 8)
                    Loop Until ModlistY.Contains(GunMod1)
                    picMod1.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod1.ToString())
                End If
            Case 3
                If ModlistR.Count = 0 Then
                    Do
                        Color1 = generator.Next(1, 5)
                    Loop Until Color1 <> "3" And Color1 <> Color2 And Color1 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod1 = generator.Next(1, 11)
                    Loop Until ModlistR.Contains(GunMod1)
                    picMod1.Image = My.Resources.ResourceManager.GetObject("red_" & GunMod1.ToString())
                End If
            Case 4
                If ModlistB.Count = 0 Then
                    Do
                        Color1 = generator.Next(1, 5)
                    Loop Until Color1 <> "4" And Color1 <> Color2 And Color1 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod1 = generator.Next(1, 10)
                    Loop Until ModlistB.Contains(GunMod1)
                    picMod1.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod1.ToString())
                End If
            Case 5
                picMod1.Image = My.Resources.Empty
        End Select
        'Color2
        My.Settings.Reload()
        Select Case Color2
            Case 1
                If ModlistP.Count = 0 Then
                    Do
                        Color2 = generator.Next(1, 5)
                    Loop Until Color2 <> "1" And Color2 <> Color1 And Color2 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod2 = generator.Next(1, 6)
                    Loop Until ModlistP.Contains(GunMod2)
                    picMod2.Image = My.Resources.ResourceManager.GetObject("purple_" & GunMod2.ToString())
                End If
            Case 2
                If ModlistY.Count = 0 Then
                    Do
                        Color2 = generator.Next(1, 5)
                    Loop Until Color2 <> "2" And Color2 <> Color1 And Color2 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod2 = generator.Next(1, 8)
                    Loop Until ModlistY.Contains(GunMod2)
                    picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                End If
            Case 3
                If ModlistR.Count = 0 Then
                    Do
                        Color2 = generator.Next(1, 5)
                    Loop Until Color2 <> "3" And Color2 <> Color1 And Color2 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod2 = generator.Next(1, 11)
                    Loop Until ModlistR.Contains(GunMod2)
                    picMod2.Image = My.Resources.ResourceManager.GetObject("red_" & GunMod2.ToString())
                End If
            Case 4
                If ModlistB.Count = 0 Then
                    Do
                        Color2 = generator.Next(1, 5)
                    Loop Until Color2 <> "4" And Color2 <> Color1 And Color2 <> Color3
                    GoTo ColorAgain
                Else
                    Do
                        GunMod2 = generator.Next(1, 10)
                    Loop Until ModlistB.Contains(GunMod2)
                    picMod2.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod2.ToString())
                End If
            Case 5
                picMod2.Image = My.Resources.Empty
        End Select
        'Color3
        My.Settings.Reload()
        Select Case Color3
            Case 1
                If ModlistP.Count = 0 Then
                    Do
                        Color3 = generator.Next(1, 5)
                    Loop Until Color3 <> "1" And Color3 <> Color1 And Color3 <> Color2
                    GoTo ColorAgain
                Else
                    Do
                        GunMod3 = generator.Next(1, 6)
                    Loop Until ModlistP.Contains(GunMod3)
                    picMod3.Image = My.Resources.ResourceManager.GetObject("purple_" & GunMod3.ToString())
                End If
            Case 2
                If ModlistY.Count = 0 Then
                    Do
                        Color3 = generator.Next(1, 5)
                    Loop Until Color3 <> "2" And Color3 <> Color1 And Color3 <> Color2
                    GoTo ColorAgain
                Else
                    Do
                        GunMod3 = generator.Next(1, 8)
                    Loop Until ModlistY.Contains(GunMod3)
                    picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                End If
            Case 3
                If ModlistR.Count = 0 Then
                    Do
                        Color3 = generator.Next(1, 5)
                    Loop Until Color3 <> "3" And Color3 <> Color1 And Color3 <> Color2
                    GoTo ColorAgain
                Else
                    Do
                        GunMod3 = generator.Next(1, 11)
                    Loop Until ModlistR.Contains(GunMod3)
                    picMod3.Image = My.Resources.ResourceManager.GetObject("red_" & GunMod3.ToString())
                End If
            Case 4
                If ModlistB.Count = 0 Then
                    Do
                        Color3 = generator.Next(1, 5)
                    Loop Until Color3 <> "4" And Color3 <> Color1 And Color3 <> Color2
                    GoTo ColorAgain
                Else
                    Do
                        GunMod3 = generator.Next(1, 10)
                    Loop Until ModlistB.Contains(GunMod3)
                    picMod3.Image = My.Resources.ResourceManager.GetObject("blue_" & GunMod3.ToString())
                End If
            Case 5
                picMod3.Image = My.Resources.Empty
            Case Else
                picMod3.Image = My.Resources.None
        End Select

        'Weapon Randomization
        'Loop Prevention
        If StatWeapon = True Then
            If My.Settings.WeapAssNo = 1 Then
                WeaponCategoryInt1 = 1
            Else : WeaponCategoryInt1 = 0
            End If
            If My.Settings.WeapLMGNo = 1 Then
                WeaponCategoryInt2 = 2
            Else : WeaponCategoryInt2 = 0
            End If
            If My.Settings.WeapRifleNo = 1 Then
                WeaponCategoryInt3 = 3
            Else : WeaponCategoryInt3 = 0
            End If
            If My.Settings.WeapShotNo = 1 Then
                WeaponCategoryInt4 = 4
            Else : WeaponCategoryInt4 = 0
            End If
            If My.Settings.WeapSMGNo = 1 Then
                WeaponCategoryInt5 = 5
            Else : WeaponCategoryInt5 = 0
            End If
            If My.Settings.WeapSniperNo = 1 Then
                WeaponCategoryInt6 = 6
            Else : WeaponCategoryInt6 = 0
            End If
            If My.Settings.WeapLegNo = 1 Then
                WeaponCategoryInt7 = 7
            Else : WeaponCategoryInt7 = 0
            End If
            If My.Settings.WeapExNo = 1 Then
                WeaponCategoryInt8 = 8
            Else : WeaponCategoryInt8 = 0
            End If

            If WeaponCategoryInt1 = 1 And WeaponCategoryInt2 = 2 And WeaponCategoryInt3 = 3 And WeaponCategoryInt4 = 4 And WeaponCategoryInt5 = 5 And _
            WeaponCategoryInt6 = 6 And WeaponCategoryInt7 = 7 And WeaponCategoryInt8 = 8 Then
                WeaponCategory = 0
                GoTo IgnoreWeapon

            Else
                Do
                    WeaponCategory = generator.Next(1, 9)
                Loop Until WeaponCategory <> WeaponCategoryInt1 And WeaponCategory <> WeaponCategoryInt2 And WeaponCategory <> WeaponCategoryInt3 And _
                    WeaponCategory <> WeaponCategoryInt4 And WeaponCategory <> WeaponCategoryInt5 And WeaponCategory <> WeaponCategoryInt6 And _
                    WeaponCategory <> WeaponCategoryInt7 And WeaponCategory <> WeaponCategoryInt8
            End If

            Select Case WeaponCategory

                Case 1 'Assault Rifles
                    GunList.Clear()
                    If My.Settings.WeapAss1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapAss2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapAss3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapAss4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapAss5 = 1 Then
                        GunList.Add(5)
                    End If
                    If My.Settings.WeapAss6 = 1 Then
                        GunList.Add(6)
                    End If
                    If My.Settings.WeapAss7 = 1 Then
                        GunList.Add(7)
                    End If
                    If My.Settings.WeapAss8 = 1 Then
                        GunList.Add(8)
                    End If
                    If My.Settings.WeapAss9 = 1 Then
                        GunList.Add(9)
                    End If
                    If My.Settings.WeapAss10 = 1 Then
                        GunList.Add(10)
                    End If
                    If My.Settings.WeapAss11 = 1 Then
                        GunList.Add(11)
                    End If
                    'If My.Settings.WeapAss12 = 1 Then
                    '    GunList.Add(12)
                    'End If

                    Do
                        Primary = generator.Next(1, 12)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("asrifle_" & Primary.ToString())
                Case 2 'LMGs
                    GunList.Clear()
                    If My.Settings.WeapLMG1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapLMG2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapLMG3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapLMG4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapLMG5 = 1 Then
                        GunList.Add(5)
                    End If
                    If My.Settings.WeapLMG6 = 1 Then
                        GunList.Add(6)
                    End If
                    If My.Settings.WeapLMG7 = 1 Then
                        GunList.Add(7)
                    End If
                    'If My.Settings.WeapLMG8 = 1 Then
                    '    GunList.Add(8)
                    'End If
                    Do
                        Primary = generator.Next(1, 8)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("lmg_" & Primary.ToString())
                Case 3 'Rifles
                    GunList.Clear()
                    If My.Settings.WeapRifle1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapRifle2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapRifle3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapRifle4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapRifle5 = 1 Then
                        GunList.Add(5)
                    End If
                    If My.Settings.WeapRifle6 = 1 Then
                        GunList.Add(6)
                    End If
                    'If My.Settings.WeapRifle7 = 1 Then
                    '    GunList.Add(7)
                    'End If
                    Do
                        Primary = generator.Next(1, 7)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("rifle_" & Primary.ToString())
                Case 4 'Shotguns
                    GunList.Clear()
                    If My.Settings.WeapShot1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapShot2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapShot3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapShot4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapShot5 = 1 Then
                        GunList.Add(5)
                    End If
                    'If My.Settings.WeapShot6 = 1 Then
                    '    GunList.Add(6)
                    'End If
                    Do
                        Primary = generator.Next(1, 6)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("shotgun_" & Primary.ToString())
                Case 5 'SMGs
                    GunList.Clear()
                    If My.Settings.WeapSMG1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapSMG2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapSMG3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapSMG4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapSMG5 = 1 Then
                        GunList.Add(5)
                    End If
                    If My.Settings.WeapSMG6 = 1 Then
                        GunList.Add(6)
                    End If
                    If My.Settings.WeapSMG7 = 1 Then
                        GunList.Add(7)
                    End If
                    If My.Settings.WeapSMG8 = 1 Then
                        GunList.Add(8)
                    End If
                    If My.Settings.WeapSMG9 = 1 Then
                        GunList.Add(9)
                    End If
                    If My.Settings.WeapSMG10 = 1 Then
                        GunList.Add(10)
                    End If
                    'If My.Settings.WeapSMG11 = 1 Then
                    '    GunList.Add(11)
                    'End If
                    Do
                        Primary = generator.Next(1, 11)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("smg_" & Primary.ToString())
                Case 6 'Snipers
                    GunList.Clear()
                    If My.Settings.WeapSniper1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapSniper2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapSniper3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapSniper4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapSniper5 = 1 Then
                        GunList.Add(5)
                    End If
                    'If My.Settings.WeapSniper6 = 1 Then
                    '    GunList.Add(6)
                    'End If
                    Do
                        Primary = generator.Next(1, 6)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("sniper_" & Primary.ToString())
                Case 7 'Legendaries
                    GunList.Clear()
                    If My.Settings.WeapLegPrim1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapLegPrim2 = 1 Then
                        GunList.Add(2)
                    End If
                    If My.Settings.WeapLegPrim3 = 1 Then
                        GunList.Add(3)
                    End If
                    If My.Settings.WeapLegPrim4 = 1 Then
                        GunList.Add(4)
                    End If
                    If My.Settings.WeapLegPrim5 = 1 Then
                        GunList.Add(5)
                    End If
                    If My.Settings.WeapLegPrim6 = 1 Then
                        GunList.Add(6)
                    End If
                    If My.Settings.WeapLegPrim7 = 1 Then
                        GunList.Add(7)
                    End If
                    If My.Settings.WeapLegPrim8 = 1 Then
                        GunList.Add(8)
                    End If
                    If My.Settings.WeapLegPrim9 = 1 Then
                        GunList.Add(9)
                    End If
                    'If My.Settings.WeapLeg13 = 1 Then
                    '    GunList.Add(13)
                    'End If
                    Do
                        Primary = generator.Next(1, 13)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("legendaryprim_" & Primary.ToString())
                Case 8 'Explosives
                    GunList.Clear()
                    If My.Settings.WeapEx1 = 1 Then
                        GunList.Add(1)
                    End If
                    If My.Settings.WeapEx2 = 1 Then
                        GunList.Add(2)
                    End If
                    'If My.Settings.WeapLeg15 = 1 Then
                    '    GunList.Add(3)
                    'End If
                    'If My.Settings.WeapLeg16 = 1 Then
                    '    GunList.Add(4)
                    'End If
                    'If My.Settings.WeapEx3 = 1 Then
                    '    GunList.Add(5)
                    'End If
                    Do
                        Primary = generator.Next(1, 3)
                    Loop Until GunList.Contains(Primary)
                    GunList.Clear()
                    picWeapon.Image = My.Resources.ResourceManager.GetObject("explosive_" & Primary.ToString())
                Case Else
                    MsgBox("Error, something went horribly wrong! A team of highly professional edgy quickswitchers is trying to resolve the issue, please restart the program for your own safety!")
            End Select
        Else : picWeapon.Image = My.Resources.Empty
        End If
IgnoreWeapon:

        'Secondary Randomization
        If StatSec = True Then
            GunList.Clear()
            If My.Settings.Sec1 = 1 Then
                GunList.Add(1)
            End If
            If My.Settings.Sec2 = 1 Then
                GunList.Add(2)
            End If
            If My.Settings.Sec3 = 1 Then
                GunList.Add(3)
            End If
            If My.Settings.Sec4 = 1 Then
                GunList.Add(4)
            End If
            If My.Settings.Sec5 = 1 Then
                GunList.Add(5)
            End If
            If My.Settings.Sec6 = 1 Then
                GunList.Add(6)
            End If
            If My.Settings.Sec7 = 1 Then
                GunList.Add(7)
            End If
            If My.Settings.Sec8 = 1 Then
                GunList.Add(8)
            End If
            If My.Settings.Sec9 = 1 Then
                GunList.Add(9)
            End If
            If My.Settings.Sec10 = 1 Then
                GunList.Add(10)
            End If
            If My.Settings.Sec11 = 1 Then
                GunList.Add(11)
            End If
            If My.Settings.Sec12 = 1 Then
                GunList.Add(12)
            End If
            If My.Settings.Sec13 = 1 Then
                GunList.Add(13)
            End If
            If My.Settings.WeapLegSec1 = 1 Then
                GunList.Add(14)
            End If
            If My.Settings.WeapLegSec2 = 1 Then
                GunList.Add(15)
            End If
            If My.Settings.WeapLegSec3 = 1 Then
                GunList.Add(16)
            End If
            If My.Settings.WeapLegSec4 = 1 Then
                GunList.Add(17)
            End If
            If My.Settings.WeapLegSec5 = 1 Then
                GunList.Add(18)
            End If
            'If My.Settings.Sec13 = 1 Then
            '    GunList.Add(19)
            'End If
            If GunList.Count = 0 Then
                picSecondary.Image = My.Resources.Empty
            Else
                Do
                    Secondary = generator.Next(1, 19)
                Loop Until GunList.Contains(Secondary)
                Select Case Secondary
                    Case 14
                        picSecondary.Image = My.Resources.legendarysec_1
                    Case 15
                        picSecondary.Image = My.Resources.legendarysec_2
                    Case 16
                        picSecondary.Image = My.Resources.legendarysec_3
                    Case 17
                        picSecondary.Image = My.Resources.legendarysec_4
                    Case 18
                        picSecondary.Image = My.Resources.legendarysec_5
                    Case Else
                        picSecondary.Image = My.Resources.ResourceManager.GetObject("secondary_" & Secondary.ToString())
                End Select
                GunList.Clear()
            End If
        End If

        'Nade Randomization
        If StatNade = True Then
            NadeList.Clear()
            If My.Settings.Nade1 = 1 Then
                NadeList.Add(1)
            End If
            If My.Settings.Nade2 = 1 Then
                NadeList.Add(2)
            End If
            If My.Settings.Nade3 = 1 Then
                NadeList.Add(3)
            End If
            If My.Settings.Nade4 = 1 Then
                NadeList.Add(4)
            End If
            If My.Settings.Nade5 = 1 Then
                NadeList.Add(5)
            End If
            If My.Settings.Nade6 = 1 Then
                NadeList.Add(6)
            End If
            If My.Settings.Nade7 = 1 Then
                NadeList.Add(7)
            End If
            'If My.Settings.Nade8 = 1 Then
            '    NadeList.Add(8)
            'End If
            If NadeList.Count = 0 Then
                picNade.Image = My.Resources.Empty
            Else : Do
                    Nade = generator.Next(1, 8)
                Loop Until NadeList.Contains(Nade)
                picNade.Image = My.Resources.ResourceManager.GetObject("nade_" & Nade.ToString())
            End If
            NadeList.Clear()
        End If

        'CharMod Green Randomization
        If StatCharGreen = True Then
            CharList.Clear()
            If My.Settings.CharGreen1 = 1 Then
                CharList.Add(1)
            End If
            If My.Settings.CharGreen2 = 1 Then
                CharList.Add(2)
            End If
            If My.Settings.CharGreen3 = 1 Then
