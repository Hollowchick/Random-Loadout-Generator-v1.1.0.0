                        ChallengeLabel2.Text = "Only do objectives in the last minute"
                    Case 21
                        ChallengeLabel1.Text = "I'll never learn"
                        ChallengeLabel2.Text = "Always approach a spot from the same direction"
                    Case 22
                        ChallengeLabel1.Text = "SPEAK UP!"
                        ChallengeLabel2.Text = "Turn your game volume to 0"
                    Case 23
                        ChallengeLabel1.Text = "What are you, a camper?"
                        ChallengeLabel2.Text = "Constantly switch positions every 10 seconds"
                    Case 24
                        ChallengeLabel1.Text = "I don't have my license yet"
                        ChallengeLabel2.Text = "You can only carsurf or be a passenger, no driving"
                    Case 25
                        ChallengeLabel1.Text = "Get off the sidewalk you maniac!"
                        ChallengeLabel2.Text = "Only drive on the road, no sidewalk or back alleys!"
