    Private Sub TabPage15_MouseMove(ByVal sender As System.Object, ByVal e As System.Windows.Forms.MouseEventArgs) Handles TabPage15.MouseMove
        My.Settings.Reload()
        If My.Settings.WeapLegPrim1 = 0 And My.Settings.WeapLegPrim2 = 0 And My.Settings.WeapLegPrim3 = 0 And My.Settings.WeapLegPrim4 = 0 And My.Settings.WeapLegPrim5 = 0 And My.Settings.WeapLegPrim6 = 0 _
         And My.Settings.WeapLegPrim7 = 0 And My.Settings.WeapLegPrim8 = 0 And My.Settings.WeapLegPrim9 = 0 Then
            My.Settings.WeapLegNo = 1
        Else : My.Settings.WeapLegNo = 0
        End If
        If My.Settings.Sec1 = 0 And My.Settings.Sec2 = 0 And My.Settings.Sec3 = 0 And My.Settings.Sec4 = 0 And My.Settings.Sec5 = 0 And My.Settings.Sec6 = 0 _
         And My.Settings.Sec7 = 0 And My.Settings.Sec8 = 0 And My.Settings.Sec9 = 0 And My.Settings.Sec10 = 0 And My.Settings.Sec11 = 0 _
         And My.Settings.Sec12 = 0 And My.Settings.Sec13 = 0 And My.Settings.WeapLegSec1 = 0 And My.Settings.WeapLegSec2 = 0 And My.Settings.WeapLegSec3 = 0 _
         And My.Settings.WeapLegSec4 = 0 And My.Settings.WeapLegSec5 = 0 Then
            My.Settings.SecNo = 1
        Else : My.Settings.SecNo = 0
        End If
        'If My.Settings.WeapEx1 = 0 And My.Settings.WeapEx2 = 0 And My.Settings.WeapLegPrim10 = 0 And My.Settings.WeapLegPrim11 = 0 Then
        '    My.Settings.WeapExNo = 1
        'Else : My.Settings.WeapExNo = 0
        'End If
        My.Settings.Save()
    End Sub

