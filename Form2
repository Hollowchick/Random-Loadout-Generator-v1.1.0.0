    End Sub

    'Hardmode Picture Change
    Private Sub picHardMode_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picHardMode.Click
        My.Settings.Reload()
        If My.Settings.HardMode = 0 Then
            picHardMode.Image = My.Resources.hardmodeselected
            My.Settings.HardMode = 1
            MsgBox("Warning! Hardmode is very experimental and it currently ignores all settings made above, use at own risk!", , ("Warning!"))
        Else
            picHardMode.Image = My.Resources.hardmode
            My.Settings.HardMode = 0
        End If
        My.Settings.Save()
    End Sub

End Class
