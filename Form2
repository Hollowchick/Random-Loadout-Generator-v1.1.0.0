            My.Settings.CharGreenNo = 1
        Else : My.Settings.CharGreenNo = 0
        End If
        If My.Settings.CharBlue1 = 0 And My.Settings.CharBlue2 = 0 And My.Settings.CharBlue3 = 0 And My.Settings.CharBlue4 = 0 And My.Settings.CharBlue5 = 0 And My.Settings.CharBlue6 = 0 Then
            My.Settings.CharBlueNo = 1
