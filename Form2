    'No R195 Mods
    Private Sub ButtonNoR195_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ButtonNoR195.Click
        My.Settings.Reload()
        'Purple Mods Out
        My.Settings.ModsP4 = 0
        picModP4.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.ModsP5 = 0
        picModP5.BackgroundImage = My.Resources.Background_ModBorderThick
        'Yellow Mods Out
        My.Settings.ModsY1 = 0
        picModY1.BackgroundImage = My.Resources.Background_ModBorderThick
        'Red Mods Out
        My.Settings.ModsR10 = 0
        picModR10.BackgroundImage = My.Resources.Background_ModBorderThick
        'Secondaries Out
        My.Settings.Sec11 = 0
        picSecondary11.BackgroundImage = My.Resources.Background_ModBorderThick
        'Nades Out
        My.Settings.Nade3 = 0
        picNade3.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.Nade4 = 0
        picNade4.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.Nade5 = 0
        picNade5.BackgroundImage = My.Resources.Background_ModBorderThick
        'Charmods Out
        My.Settings.CharGreen4 = 0
        picCharGreen4.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.CharGreen5 = 0
        picCharGreen5.BackgroundImage = My.Resources.Background_ModBorderThick

        My.Settings.CharBlue4 = 0
        picCharBlue4.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.CharBlue5 = 0
        picCharBlue5.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.CharBlue6 = 0
        picCharBlue6.BackgroundImage = My.Resources.Background_ModBorderThick
        My.Settings.Save()
    End Sub

    'Empty Mods Picture Change
    Private Sub picEmptyMods_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picEmptyMods.Click
        My.Settings.Reload()
        If My.Settings.EmptyMods = 0 Then
            picEmptyMods.Image = My.Resources.emptymodsselected
            My.Settings.EmptyMods = 1
        Else
            picEmptyMods.Image = My.Resources.emptymods
            My.Settings.EmptyMods = 0
        End If
        My.Settings.Save()
    End Sub

    'Challenges Picture Change
    Private Sub picChallenges_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picChallenges.Click
        My.Settings.Reload()
