        Else : picCharBlue5.BackgroundImage = My.Resources.Background_ModBorderThick
        End If
        If My.Settings.CharBlue6 = 1 Then
            picCharBlue6.BackgroundImage = My.Resources.Background_ModBorderSelectedThick
        Else : picCharBlue6.BackgroundImage = My.Resources.Background_ModBorderThick
        End If
        'Empty Mods Load
        If My.Settings.EmptyMods = 1 Then
            picEmptyMods.Image = My.Resources.emptymodsselected
        Else : picEmptyMods.Image = My.Resources.emptymods
        End If
        'Challenges Load
        If My.Settings.Challenges = 1 Then
            picChallenges.Image = My.Resources.challengesselected
        Else : picChallenges.Image = My.Resources.challenges
        End If
        'Hard Mode Load
        If My.Settings.HardMode = 1 Then
            picHardMode.Image = My.Resources.hardmodeselected
        Else : picHardMode.Image = My.Resources.hardmode
        End If
    End Sub
